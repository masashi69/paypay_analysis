
WindowManager:
    Main:
    Analized:

<Main>:
    name: 'main'
    id: main

    BoxLayout:
        orientation: 'vertical'
        Label:
            text: 'Top page'
            id: label
        Button:
            text: 'Select import file'
            on_release: main.open(fc.path, fc.selection)
        Button:
            text: 'Select table'
            on_release: main.createdb(main.open(fc.path, fc.selection))
        FileChooserListView:
            id: fc
            path: '../ドキュメント/paypay/'
            filters: ['*.csv']
            #on_selection: main.selected(fc.selection)


<Analized>:
    name: 'analize'

    BoxLayout:
        orientation: 'vertical'
        Label:
            text: ''
            id: result


